<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <td>Title</td>
      <td>Body</td>
      <td>User's e-mail</td>
      <td>Actions</td>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post|  %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.body %></td>
        <td><%= post.user.email %></td>
        <td><p>Some actions</p></td>
      </tr>
    <% end %>
  </tbody>
</table>